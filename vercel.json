
{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/service-worker.js",
      "destination": "/service-worker.js"
    },
    {
      "source": "/manifest.json",
      "destination": "/manifest.json"
    },
    {
      "source": "/icon.svg",
      "destination": "/icon.svg"
    },
    {
      "source": "/index.tsx",
      "destination": "/index.tsx"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
